{
	"name": "otne",
	"displayName": "Otne",
	"description": "Otne Language",
	"version": "0.0.4",
	"icon": "otne.png",
	"author": "beanflame",
	"publisher": "uxnt",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Programming Languages"
	],
	"repository": {
		"url": "https://github.com/uxnt/vscode-otne"
	},
	"contributes": {

		"iconThemes": [
			{
				"id": "otne-icon-theme",
				"label": "Otne Icon Theme",
				"path": "./dist/otne-icons.json",
				"_watch": true
			}
		],

		"languages": [
			{
				"id": "otne",
				"aliases": [
					"Otne",
					"otne"
				],
				"extensions": [
					".otne"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "otne",
				"scopeName": "source.otne",
				"path": "./syntaxes/otne.tmLanguage.json"
			}
		]
	}
}
